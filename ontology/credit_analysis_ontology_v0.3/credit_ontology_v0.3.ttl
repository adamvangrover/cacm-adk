esg:CarbonEmission rdf:type owl:Class ;
    rdfs:subClassOf esg:EnvironmentalFactor ;
    rdfs:label "Carbon Emission"^^xsd:string ;
    rdfs:comment "Metric for carbon emissions, detailing value, unit, reporting period, and scope."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:WaterUsage rdf:type owl:Class ;
    rdfs:subClassOf esg:EnvironmentalFactor ;
    rdfs:label "Water Usage"^^xsd:string ;
    rdfs:comment "Metric for water usage by an entity."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:WasteManagementPolicyAdherence rdf:type owl:Class ; # This might be more of a qualitative rating or status
    rdfs:subClassOf esg:EnvironmentalFactor ;
    rdfs:label "Waste Management Policy Adherence"^^xsd:string ;
    rdfs:comment "Adherence to waste management policies, potentially a rating or status."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:EmployeeSafetyRecord rdf:type owl:Class ; # Could be a set of metrics or a rating
    rdfs:subClassOf esg:SocialFactor ;
    rdfs:label "Employee Safety Record"^^xsd:string ;
    rdfs:comment "Record or rating related to employee safety."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:CommunityInvestmentLevel rdf:type owl:Class ;
    rdfs:subClassOf esg:SocialFactor ;
    rdfs:label "Community Investment Level"^^xsd:string ;
    rdfs:comment "Level of investment in community initiatives."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:DiversityAndInclusionRating rdf:type owl:Class ;
    rdfs:subClassOf esg:SocialFactor ;
    rdfs:label "Diversity and Inclusion Rating"^^xsd:string ;
    rdfs:comment "Rating of an entity's diversity and inclusion performance."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:BoardIndependenceRatio rdf:type owl:Class ;
    rdfs:subClassOf esg:GovernanceFactor ;
    rdfs:subClassOf cacm_ont:Ratio ; # It's also a ratio
    rdfs:label "Board Independence Ratio"^^xsd:string ;
    rdfs:comment "Ratio of independent directors on the board."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:ExecutiveCompensationPolicyTransparency rdf:type owl:Class ; # Could be a rating or status
    rdfs:subClassOf esg:GovernanceFactor ;
    rdfs:label "Executive Compensation Policy Transparency"^^xsd:string ;
    rdfs:comment "Transparency of executive compensation policies."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:ShareholderRightsStrength rdf:type owl:Class ; # Could be a rating or score
    rdfs:subClassOf esg:GovernanceFactor ;
    rdfs:label "Shareholder Rights Strength"^^xsd:string ;
    rdfs:comment "Strength of shareholder rights within the company."^^xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:OverallESGRating rdf:type owl:Class;
  rdfs:subClassOf kgclass:Rating ;
  rdfs:label "Overall ESG Rating"^^xsd:string ;
  rdfs:comment "An overall rating assessing an entity's ESG performance, often provided by third-party agencies."^^xsd:string ;
  rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .


# --- ESG Data Properties (Datatype Properties) ---

esg:metricValue rdf:type owl:DatatypeProperty ; # Generic value for any ESG metric
    rdfs:label "Metric Value"^^xsd:string ;
    rdfs:comment "The value of an ESG metric."^^xsd:string ;
    rdfs:domain esg:ESGMetric ; # Apply to parent ESGMetric for inheritance
    rdfs:range xsd:string ; # General, can be refined by specific metric types or instances (e.g. float for CarbonEmissionValue)
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:metricUnit rdf:type owl:DatatypeProperty ;
    rdfs:label "Metric Unit"^^xsd:string ;
    rdfs:comment "The unit of measurement for an ESG metric (e.g., 'tonnes CO2e', 'cubic meters', '%')."^^xsd:string ;
    rdfs:domain esg:ESGMetric ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:reportingPeriod rdf:type owl:DatatypeProperty ;
    rdfs:label "Reporting Period"^^xsd:string ;
    rdfs:comment "The period to which an ESG metric applies (e.g., '2023', 'Q1 2024')."^^xsd:string ;
    rdfs:domain esg:ESGMetric ;
    rdfs:range xsd:string ; # Could be more specific like gYear or a custom date range type
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:emissionScope rdf:type owl:DatatypeProperty ;
    rdfs:label "Emission Scope"^^xsd:string ;
    rdfs:comment "Scope of carbon emissions (e.g., 'Scope 1', 'Scope 2', 'Scope 3')."^^xsd:string ;
    rdfs:domain esg:CarbonEmission ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:ratingValue rdf:type owl:DatatypeProperty ; # For qualitative ratings that are ESG specific
    rdfs:label "ESG Rating Value"^^xsd:string ;
    rdfs:comment "Value of an ESG rating (e.g., 'AAA', 'B', 'High', 'Compliant')."^^xsd:string ;
    rdfs:domain esg:WasteManagementPolicyAdherence ;
    rdfs:domain esg:EmployeeSafetyRecord ;
    rdfs:domain esg:DiversityAndInclusionRating ;
    rdfs:domain esg:ExecutiveCompensationPolicyTransparency ;
    rdfs:domain esg:ShareholderRightsStrength ;
    rdfs:domain esg:OverallESGRating ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

esg:dataSource rdf:type owl:DatatypeProperty ; # Could also be an object property if linking to a Data Source class
    rdfs:label "ESG Data Source"^^xsd:string ;
    rdfs:comment "Source of the ESG data or rating (e.g., 'Company Report', 'MSCI', 'Sustainalytics')."^^xsd:string ;
    rdfs:domain esg:ESGMetric ;
    rdfs:domain esg:OverallESGRating ;
    rdfs:range xsd:string ;
    rdfs:isDefinedBy <http://example.com/ontology/cacm_credit_ontology/0.3#> .

